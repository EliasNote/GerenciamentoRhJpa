<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<FlowPane prefHeight="205.0" prefWidth="317.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.esand.GerenciamentoRh.LoginController">
    <children>
        <VBox alignment="CENTER" prefHeight="228.0" prefWidth="315.0" spacing="20.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="58.0" text="LOGIN" textAlignment="CENTER">
                    <font>
                        <Font name="Segoe UI Bold" size="16.0" />
                    </font>
                </Label>
                <GridPane prefHeight="60.0" prefWidth="278.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="158.0" minWidth="10.0" prefWidth="66.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="245.0" minWidth="10.0" prefWidth="209.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label prefHeight="17.0" prefWidth="23.0" text="CPF" />
                        <Label text="Senha" GridPane.rowIndex="1" />
                        <TextField fx:id="CPF" GridPane.columnIndex="1" />
                        <TextField fx:id="Senha" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                    </children>
                </GridPane>
                <Button mnemonicParsing="false" onAction="#logar" prefHeight="30.0" prefWidth="85.0" text="Acessar" />
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </VBox>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</FlowPane>
